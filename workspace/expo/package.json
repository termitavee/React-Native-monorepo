{
  "name": "@workspace/expo",
  "main": "./src/entry-point.ts",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "expo start",
    "open:android": "expo run:android",
    "open:ios": "expo run:ios",
    "open:web": "expo run:web",
    "open:desktop": "electron .",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform android --local",
    "build:web": "npx expo export -p web",
    "build:desktop": "npm run build:web && echo // TODO investigate electron-forge",
    "lint": "TIMING=1 eslint \"./src/**\"",
    "lint:silent": "npm run lint --quiet",
    "test": "TZ=UTC jest --passWithNoTests",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "web": "expo run:web"
  },
  "dependencies": {
    "@craco/craco": "5.9.0",
    "electron": "26.6.1",
    "expo": "^49.0.21",
    "expo-splash-screen": "^0.22.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "^19.0.0",
    "@types/react": "~18.2.14"
  },
  "expo": {
    "entryPoint": "./src/entry-point.ts"
  }
}
